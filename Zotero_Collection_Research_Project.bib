@article{lebedevQuaternaryVolcanicRocks2013,
  title = {The Quaternary Volcanic Rocks of the {{Geghama}} Highland, {{Lesser Caucasus}}, {{Armenia}}: {{Geochronology}}, Isotopic {{Sr}}-{{Nd}} Characteristics, and Origin},
