  doi = {10.1016/j.jas.2014.10.014},
  author = {Messager, Erwan and Herrscher, Estelle and Martin, Lucie and Kvavadze, Eliso and Martkoplishvili, Inga and Delhon, Claire and Kakhiani, Kakha and Bedianashvili, Giorgi and Sagona, Antonio and Bitadze, Liana and Poulmarc'h, Modwene and Guy, Andr{\'e} and Lordkipanidze, David},
  month = jan,
  year = {2015},
  keywords = {Early Bronze Age,South Caucasus,Stable isotope analysis,Farming activities,Human and animal skeletons,Plant macroremains,Pollens and phytoliths},
  pages = {214-226},
  file = {C:\\Users\\alexs\\Zotero\\storage\\HWMJ4D3S\\Messager et al. - 2015 - Archaeobotanical and isotopic evidence of Early Br.pdf;C:\\Users\\alexs\\Zotero\\storage\\BUN7HMRS\\S0305440314003847.html}
}

@article{lebedevOriginAdakiteLikeMagmas2019,
