@article{messagerArchaeobotanicalIsotopicEvidence2015,
  title = {Archaeobotanical and Isotopic Evidence of {{Early Bronze Age}} Farming Activities and Diet in the Mountainous Environment of the {{South Caucasus}}: A Pilot Study of {{Chobareti}} Site ({{Samtskhe}}\textendash{{Javakheti}} Region)},
